.min{
    background:#fff;
    padding:10px 0 10px 0;
    margin-top:30rpx;
}
.message-list{
    background: #fff;
    margin:0 40rpx;
    position: relative;
    border-left:1px solid #eee;
}
.message-time{
    font-size: 15px;
    color: #000;
    line-height: 20px;
    padding-left: 30rpx;
    position: relative;
}
.message-time image{
    width: 30rpx;
    height: 30rpx;
    position: absolute;
    left:-15rpx;
    top:5rpx;
    z-index: 10;
}
.message-map{
    position: relative;
}
.message-map image{
    width:20rpx;
    height:20rpx;
    position:absolute;
    left:-10rpx;
    top:21rpx;
    z-index:10;
}
.message-icon,.message-map{
    font-size: 14px;
    color: #000000;
    padding-top: 8px;
    padding-left:16px;
}
.message-icon em{
    color: #ff3535;
}