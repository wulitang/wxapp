/*技能评价*/
.skill{
   background: #FFF;
   margin-top: 18px;
   height: 202px;
   margin-bottom: 40px;
   padding-left: 15px;
}
.skill_t {
   padding-right: 10px;
   line-height: 24px;
   margin-bottom: 15px; 
  }
.skill_t label{
  font-size: 14px;
  float: left;
  color: #2f2f2f;
}
.skill_t text{
 font-size: 14px;
 float: right;
 color: #8b8b8b;
}
.skill_h {  
  background: #ebebeb;
  border-radius: 10px;
  width: 100%;
  height: 8px;
  overflow: hidden;

}
.skill_b{
  
 position: relative;
}
.skill_b::before{
content: '';
position: absolute;
top: 0;
bottom: 0;
left: 0;
right: 0;
z-index: 1;
height: 8px;
background:#f3bf4f;
border-radius: 10px;
}