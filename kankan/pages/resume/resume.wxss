page{
     background: #f0eff5;
     font-family: "黑体";
}
view,text{font-family: "黑体";}
.resume_us{
  height: 184px;
  background: #f3bf4f;
  width: 100%;
  text-align: center;
}
.resume_us image{
 border-radius: 50%;
 width: 70px;
 height: 70px;
 margin-top: 24px;
}
.resume_us view{
 margin-top: 10px;
 overflow:hidden;
 width: 100%;
 margin-bottom: 10px;
 font-size: 17px;
 color: #000000;

}
.resume_us text{
   color: #865d04;
   font-size: 14px;
   width: 245px;
   height: 16px;
   white-space: nowrap;
   text-overflow: ellipsis;
   overflow: hidden;
   display: block;
   margin-left: 16%;
}
@media screen and (max-width: 370px) {
.resume_us text{ margin-left: 14%;}
}
@media screen and (min-width:380px){
.resume_us text{margin-left: 20%;}
}
.resume_jb{
