label,text,view{
	font-family: "黑体";
}
.search_frame{
padding: 6px;
position: relative;
width: 100%;
height: 27px;
}
.search_area{
	float: left;
	font-size: 14px;
	color: #000;
    width: 60px;
    height: 27px;
	line-height: 27px;
	background: url(images/fhn.png) no-repeat 70% center;
	background-size: 11px;
}
.search_frame input{
 background:#fafbfd;
 width: 66%;
 padding-left: 40px;
 font-size: 12px;
 color: #bfbfbf;
 border-radius: 5px;
 float: left;
 height: 27px;
}
.search_frame label{
	 background:url(images/soo.png) no-repeat;
	 background-size: 14px;
	 width: 14px;
	 height: 14px;
	 display: block;
	 left: 76px;
	 top:12px;
	 position: absolute;

}
/*列表*/
.search_list{
	padding: 10px 0px;
	background: #FFF;
	overflow: hidden;
}
.search_li{
	margin-bottom: 10px;
	overflow: hidden;
	width: 100%;
}
.search_left{
	width: 15%;
	overflow: hidden;
	float: left;
	margin-right: 5%;
}
.search_list image{
  width: 55px;
  height: 55px;
  overflow: hidden;
  float: left;
}
.search_list {
	padding:10px 0px 10px 8px;
}
.search_list navigator{
	height: 86px;
	float: left;
	border-bottom: solid 1px #e2e2e2;
	width: 80%;
	position: relative;
}
.search_list navigator label{
	color: #010101;
	font-size: 16px;
	display: block;
}
.search_list navigator text{
 font-size: 12px;
 display: block;
 color: #a9a9a9;
 margin-bottom: 4px;
 max-width: 80%;
 height: 16px;
 white-space: nowrap;
text-overflow: ellipsis;
overflow: hidden;
}
.search_list navigator .search_sh{
	font-size: 14px;
	margin: 4px 0px 8px 0px;
}
.search_list navigator .search_price{
position: absolute;
right: 15px;
top: 5px;
color: #fc571e;
font-size: 15px;
}
.search_list navigator .search_rq{
	color: #d0d0d0;
	font-size: 12px;
	right: 15px;
	top: 40px;
	position: absolute;
}